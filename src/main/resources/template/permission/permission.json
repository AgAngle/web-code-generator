{
  "id": "${permissionPrefix}",
  "name": "permission.${moduleName}.${class_name}.name",
  "permissions": [
    {
      "id": "${permissionPrefix}:READ"
    },
    {
      "id": "${permissionPrefix}:READ+ADD"
    },
    {
      "id": "${permissionPrefix}:READ+UPDATE"
    },
    {
      "id": "${permissionPrefix}:READ+DELETE"
    }
  ]
}

